package youTube;

public class Objectpassing {

	public static void main(String[] args) {
		
		Garage g = new Garage();
		
		Car name = new Car("BMW");
		
		g.park(name);

	}

}

public class Car{
	String name;
	Car(String n){
		this.name = n;
	}
}

public class Garage{
	
	
	void park(Car car){
		System.out.println("The "+car.name+"is parking in garage");
	}
	
}

